<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	
	<bean id ="oc" class = "com.xworkz.online_classes_details.online_classes.bean.OnlineClassesBean ">
	<property name="id" value="1"></property>
	<property name="subject" value="Math"></property>
	<property name="teacherName" value="Raj"></property>
	<property name="duration" value="1"></property>
	<property name="noOfAssignments" value="2"></property>
	</bean> 	
	
	<bean id ="in" class ="java.lang.Integer ">
	<constructor-arg value="2"></constructor-arg>
	</bean>
	
	

</beans>
